@model IEnumerable<proyectoFi.Models.Categorium>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<html>
    <body>
        <a asp-action="Create">Agregar categoria</a>
        <h1>CATEGORIA</h1>

        <table class="table table-bordered">
            <tr>
                <td scope="col">Nombre</td>
                <td scope="col">Id</td>
            </tr>
        @foreach(var item in Model)
        {
            <tr>
                <td scope="col">@item.NomCategoria</td>
                <td scope="col">@item.IdCategoria</td>
            </tr>
        }
        </table>
    </body>
</html>